{
	"name": "Json2Parquet",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "JsonReadSettings"
						}
					},
					"sink": {
						"type": "ParquetSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings",
							"copyBehavior": "FlattenHierarchy"
						},
						"formatSettings": {
							"type": "ParquetWriteSettings"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "['orderId']"
								},
								"sink": {
									"name": "orderId",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['clientName']"
								},
								"sink": {
									"name": "clientName",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['invoiceOutput']"
								},
								"sink": {
									"name": "items",
									"type": "String"
								}
							}
						],
						"collectionReference": "$['list']",
						"mapComplexValuesToString": false
					}
				},
				"inputs": [
					{
						"referenceName": "Json2Parquet",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "Json2ParquetDestino",
						"type": "DatasetReference"
					}
				]
			}
		],
		"folder": {
			"name": "_Testes de Conectividade"
		},
		"annotations": []
	}
}