{
	"name": "json_2_parquet_vtex",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "JsonReadSettings"
						}
					},
					"sink": {
						"type": "ParquetSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "ParquetWriteSettings"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['orderId']"
								},
								"sink": {
									"name": "orderId",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['sequence']"
								},
								"sink": {
									"name": "sequence",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['marketplaceOrderId']"
								},
								"sink": {
									"name": "marketplaceOrderId",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['marketplaceServicesEndpoint']"
								},
								"sink": {
									"name": "marketplaceServicesEndpoint",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['sellerOrderId']"
								},
								"sink": {
									"name": "sellerOrderId",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['origin']"
								},
								"sink": {
									"name": "origin",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['affiliateId']"
								},
								"sink": {
									"name": "affiliateId",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['salesChannel']"
								},
								"sink": {
									"name": "salesChannel",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['merchantName']"
								},
								"sink": {
									"name": "merchantName",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['status']"
								},
								"sink": {
									"name": "status",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['statusDescription']"
								},
								"sink": {
									"name": "statusDescription",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['value']"
								},
								"sink": {
									"name": "value",
									"type": "Int16"
								}
							},
							{
								"source": {
									"path": "$['creationDate']"
								},
								"sink": {
									"name": "creationDate",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['lastChange']"
								},
								"sink": {
									"name": "lastChange",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['orderGroup']"
								},
								"sink": {
									"name": "orderGroup",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['id']"
								},
								"sink": {
									"name": "id_totals",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['name']"
								},
								"sink": {
									"name": "name_totals",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['value']"
								},
								"sink": {
									"name": "value_Value_totals",
									"type": "String"
								}
							}
						],
						"collectionReference": "$['totals']",
						"mapComplexValuesToString": true
					}
				},
				"inputs": [
					{
						"referenceName": "vtexj_order",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "vtex_order_parquet",
						"type": "DatasetReference"
					}
				]
			}
		],
		"folder": {
			"name": "_Testes de Conectividade"
		},
		"annotations": []
	}
}