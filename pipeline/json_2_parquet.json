{
	"name": "json_2_parquet",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "JsonReadSettings"
						}
					},
					"sink": {
						"type": "ParquetSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "ParquetWriteSettings"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['E_AUSP'][0]['OBJEK']"
								},
								"sink": {
									"name": "OBJEK",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['E_AUSP'][0]['ATINN']"
								},
								"sink": {
									"name": "ATINN",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['E_AUSP'][0]['ATZHL']"
								},
								"sink": {
									"name": "ATZHL",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['E_AUSP'][0]['MAFID']"
								},
								"sink": {
									"name": "MAFID",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['E_AUSP'][0]['KLART']"
								},
								"sink": {
									"name": "KLART",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['E_AUSP'][0]['ATWRT']"
								},
								"sink": {
									"name": "ATWRT",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['E_MENSAGEM']"
								},
								"sink": {
									"name": "E_MENSAGEM",
									"type": "String"
								}
							}
						],
						"mapComplexValuesToString": true
					}
				},
				"inputs": [
					{
						"referenceName": "origem_sap_jason",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "destino_sap_parquet",
						"type": "DatasetReference"
					}
				]
			}
		],
		"folder": {
			"name": "_Testes de Conectividade"
		},
		"annotations": []
	}
}