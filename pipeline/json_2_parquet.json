{
	"name": "json_2_parquet",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "JsonReadSettings"
						}
					},
					"sink": {
						"type": "ParquetSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "ParquetWriteSettings"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['E_MENSAGEM']"
								},
								"sink": {
									"name": "E_MENSAGEM"
								}
							},
							{
								"source": {
									"path": "['VBELN']"
								},
								"sink": {
									"name": "VBELN"
								}
							},
							{
								"source": {
									"path": "['POSNR']"
								},
								"sink": {
									"name": "POSNR"
								}
							},
							{
								"source": {
									"path": "['ETENR']"
								},
								"sink": {
									"name": "ETENR"
								}
							},
							{
								"source": {
									"path": "['WMENG']"
								},
								"sink": {
									"name": "WMENG"
								}
							},
							{
								"source": {
									"path": "['J_3ASIZE']"
								},
								"sink": {
									"name": "J_3ASIZE"
								}
							},
							{
								"source": {
									"path": "['J_3ASZGR']"
								},
								"sink": {
									"name": "J_3ASZGR"
								}
							},
							{
								"source": {
									"path": "['J_3ANETW']"
								},
								"sink": {
									"name": "J_3ANETW"
								}
							}
						],
						"collectionReference": "$['E_VBEP']",
						"mapComplexValuesToString": true
					}
				},
				"inputs": [
					{
						"referenceName": "origem_sap_jason",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "destino_sap_parquet",
						"type": "DatasetReference"
					}
				]
			}
		],
		"folder": {
			"name": "_Testes de Conectividade"
		},
		"annotations": []
	}
}