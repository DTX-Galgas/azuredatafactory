{
	"name": "teste",
	"properties": {
		"activities": [
			{
				"name": "Grava tabela total paginas ordem",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "RestSource",
						"httpRequestTimeout": "00:01:40",
						"requestInterval": "00.00:00:00.010",
						"requestMethod": "GET",
						"additionalHeaders": {
							"X-VTEX-API-AppToken": {
								"value": "@variables('AppToken')",
								"type": "Expression"
							},
							"x-vtex-api-appkey": {
								"value": "@variables('AppKey')",
								"type": "Expression"
							}
						}
					},
					"sink": {
						"type": "AzureSqlSink"
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "RestVTEX",
						"type": "DatasetReference",
						"parameters": {
							"ParRelativeURL": {
								"value": "@variables('RelativeURL')",
								"type": "Expression"
							}
						}
					}
				],
				"outputs": [
					{
						"referenceName": "SQL_DS_DW_MALWEE",
						"type": "DatasetReference",
						"parameters": {
							"NomeTabela": {
								"value": "@concat(\nactivity('TabelaTotalPaginas').output.firstRow.NOM_SCHEMA_DESTINO,\n'.',\nactivity('TabelaTotalPaginas').output.firstRow.NOM_TABELA_DESTINO\n)\n",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"folder": {
			"name": "Modelos/API JSON/VTEX"
		},
		"annotations": []
	}
}